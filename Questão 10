#include <stdio.h>

int main() {
  
   char *x[4], *x0;// endereço de char
   x0=4092;
    printf("endereço char:\n%d\n%d\n%d\n", x0+1, x0+2, x0+3);

  int *y[4], *y0;//endereço de int
   y0=4092;
    printf("endereço int:\n%d\n%d\n%d\n", y0+1, y0+2, y0+3);

  float *z[4], *z0;//endereço de float
   z0=4092;
    printf("endereço float:\n%d\n%d\n%d\n", z0+1, z0+2, z0+3);

  double *w[4], *w0;//endereço de double 
   w0=4092;
    printf("endereço double:\n%d\n%d\n%d\n", w0+1, w0+2, w0+3);
    return 0;
}
